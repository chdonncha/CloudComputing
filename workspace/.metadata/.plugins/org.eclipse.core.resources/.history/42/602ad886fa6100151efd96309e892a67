package gmail.chdonncha.com;


public class Euler1 {

	//public ArrayList<Integer> multipleOf3;
	//public ArrayList<Integer> multipleOf5;
	public int multipleOf3;
	public int multipleOf5;
	public int total = 0;
	public int index = 1000;
	
	
	while(index < 0)
	{
		if(index % 5 == 0 || index % 3 == 0)
		{
			total += index;
		}
		index --;
	}
	

}
